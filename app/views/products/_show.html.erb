



<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Description</th>
          <th>Price</th>
          <th colspan="3"></th>
        </tr>
      </thead>
    
      <tbody>
        <% @products.each do |product| %>
          <tr>
            <td><%= product.name %></td>
            <td><%= product.description %></td>
            <td>$<%= product.price %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>